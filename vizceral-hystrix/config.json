    },
    {
      "group": "languages-service",
      "cluster": "languages-service"
    },
    {
      "group": "github-service",
      "cluster": "github-service"
    },
   {
     "group": "github-api",
     "cluster": "github-api"
   },
   {
     "group": "twitter-api",
     "cluster": "twitter-api"
   }
 ]
