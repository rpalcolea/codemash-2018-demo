  "timeoutPercentageThreshold": 0,
  "failurePercentageThreshold": 0,
  "turbine": {
    "host": "turbine.codemash.org",
    "port": 80,
    "path": "/turbine.stream?cluster="
  },
  "entryClusters": [
    "language-aggregator-api",
    "github-service",
    "twitter-service"
